#
[beaver]
<% @beaver.each do |key, value| %>
<%= "#{key}: #{value}" %>
<% end %>

<% @input.each do |input_type| %>
input {
<%= input_type.first %> {
<% input_type.last.each do |key, value| %>
  <%= "#{key} => #{value}" %>
    <% end %>
  <% end %>
  }
}

<% @output.each do |key, value| %>
<%= "#{key} { #{value} }" %>
<% end %>

<% @files.each do |file| %>
[<%= file["path"] %>]
  <% file["options"].each do |key, value| %>
<%= "#{key}: #{value}" %>
  <% end %>
<% end %>
